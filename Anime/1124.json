          "name": {
            "first": "Hidetoshi",
            "last": "Oomori",
            "native": "大森英敏"
          },
          "language": "JAPANESE",
          "image": {
            "large": "https://s4.anilist.co/file/anilistcdn/staff/large/n105958-DcZcztozvNNx.png",
