      {
        "role": "Art Director",
        "node": {
          "id": 103964,
          "name": {
            "first": "Shichirou",
            "last": "Kobayashi",
            "native": "小林七郎"
          },
          "language": "JAPANESE",
          "image": {
            "large": "https://s4.anilist.co/file/anilistcdn/staff/large/n103964-1Da7Vx9y0nlo.png",
