      {
        "role": "Director",
        "node": {
          "id": 101518,
          "name": {
            "first": "Masahiro",
            "last": "Andou",
            "native": "安藤真裕"
          },
          "language": "JAPANESE",
          "image": {
            "large": "https://s4.anilist.co/file/anilistcdn/staff/large/n101518-daLPjgrbCaQi.png",
