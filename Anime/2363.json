      {
        "role": "Original Creator",
        "node": {
          "id": 100372,
          "name": {
            "first": "Satoshi",
            "last": "Tajiri",
            "native": "田尻智"
          },
          "language": "JAPANESE",
          "image": {
            "large": "https://s4.anilist.co/file/anilistcdn/staff/large/n100372-ztnZ3FtrHMlw.jpg",
            "medium": "https://s4.anilist.co/file/anilistcdn/staff/medium/n100372-ztnZ3FtrHMlw.jpg"
