          "language": "PORTUGUESE",
          "image": {
            "large": "https://s4.anilist.co/file/anilistcdn/staff/large/n106595-Cx9Afvxjg0ah.jpg",
            "medium": "https://s4.anilist.co/file/anilistcdn/staff/medium/n106595-Cx9Afvxjg0ah.jpg"
