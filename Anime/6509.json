      {
        "role": "Key Animation (eps 9, 14, 19, 25, 31, 39, 42, 45, 48, 53, 57, 63, 66, 69)",
        "node": {
          "id": 100068,
          "name": {
            "first": "Masaaki",
            "last": "Yuasa",
            "native": "湯浅政明"
          },
          "language": "JAPANESE",
          "image": {
            "large": "https://s4.anilist.co/file/anilistcdn/staff/large/n100068-6DCiwXuyBESm.png",
