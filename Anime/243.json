      {
        "role": "Original Creator",
        "node": {
          "id": 97316,
          "name": {
            "first": "Maki",
            "last": "Murakami",
            "native": "村上真紀"
          },
          "language": "JAPANESE",
          "image": {
            "large": "https://s4.anilist.co/file/anilistcdn/staff/large/n97316-putzpRblSwnF.jpg",
