        "node": {
          "id": 110607,
          "name": {
            "first": "Konomi",
            "last": "Suzuki",
            "native": "鈴木このみ"
          },
          "language": "JAPANESE",
          "image": {
            "large": "https://s4.anilist.co/file/anilistcdn/staff/large/n110607-UtSaOYgCcqBE.jpg",
